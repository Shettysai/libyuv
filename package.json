{
  "name": "libyuv",
  "version": "0.0.1",
  "keywords": [
    "video",
    "yuv",
    "conversion",
    "frames"
  ],
  "description": "Bindings to libyuv, provides conversion utilities for in-memory YUV video frames",
  "repository": {
    "url": "https://github.com/astronautlabs/libyuv-node"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "install": "npm run libyuv:build && npm run rebuild",
    "libyuv:build": "node scripts/libyuv-build.js",
    "libyuv:clean": "rimraf external/libyuv",
    "clean": "npm run libyuv:clean && rimraf build",
    "rebuild": "node-gyp rebuild",
    "build": "npm run libyuv:build && npm run rebuild",
    "prepublish": "npm run ts:build",
    "ts:build": "tsc -p .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Astronaut Labs",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "node-addon-api": "*"
  }
}
